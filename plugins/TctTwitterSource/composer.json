{
    "name": "edasubert/TctTwitterSource",
    "description": "TCT plugin for Twitter source for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.1",
        "j7mbo/twitter-api-php": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "TctTwitterSource\\": "src",
            "TwitterAPIExchange\\": "./vendor/j7mbo/twitter-api-php/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TctTwitterSource\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
